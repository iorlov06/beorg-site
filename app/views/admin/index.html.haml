%h2
  Search for user
=form_tag '/admin/search', method: :get do
  %p
    =label_tag :username
    =text_field_tag :username
  %p
    =label_tag :email
    =text_field_tag :email
  %p
    =label_tag :phone
    =text_field_tag :phone
  %p
    =label_tag :name
    =text_field_tag :name
  =submit_tag 'Search'
